#!/usr/bin/env sh

: ${GOOS:=linux}
: ${GOARCH:=amd64}

echo tar -C dist/${GOOS}/${GOARCH}/ -v --exclude *.gz -czf dist/${GOOS}/${GOARCH}/local-webserver_${GOOS}_${GOARCH}.tar.gz local-webserver
tar -C dist/${GOOS}/${GOARCH}/ -v --exclude *.gz -czf dist/${GOOS}/${GOARCH}/local-webserver_${GOOS}_${GOARCH}.tar.gz local-webserver
